{
  "name": "webpack-base",
  "version": "1.0.0",
  "description": "Webpack base configuration",
  "main": "src/index.js",
  "keywords": [],
  "author": "Nelson Daza",
  "license": "ISC",
  "browserslist": [
    "> 1%, last 3 versions",
    "not dead",
    "not IE < 11",
    "not ExplorerMobile < 11"
  ],
  "engines": {
    "node": "^16",
    "yarn": "1.x"
  },
  "scripts": {
    "build": "yarn clear:public && yarn compile",
    "build:progress": "yarn build --progress",
    "build:sentry": "yarn build --env SENTRY",
    "build:stats": "yarn build --stats",
    "build:stories": "yarn install && yarn build-storybook -c config/storybook -o dist/stories",
    "build:w": "yarn build -w",
    "clear": "shx rm -rf ./dist",
    "clear:public": "shx rm -rf ./dist/public",
    "compile": "yarn install && yarn webpack --mode production --bail",
    "cy:open": "cypress open -C ./cypress/cypress.config.ts --env envGroup=dev",
    "cy:prod:open": "cypress open -C ./cypress/cypress.config.ts --env envGroup=production",
    "cy:prod:run": "cypress run -C ./cypress/cypress.config.ts --env envGroup=production",
    "cy:public:open": "cypress open -C ./cypress/cypress.config.ts --env envGroup=public",
    "cy:public:run": "cypress run -C ./cypress/cypress.config.ts --env envGroup=public",
    "cy:run": "cypress run -C ./cypress/cypress.config.ts --env envGroup=dev",
    "cy:staging:open": "cypress open -C ./cypress/cypress.config.ts --env envGroup=staging",
    "cy:staging:run": "cypress run -C ./cypress/cypress.config.ts --env envGroup=staging",
    "deploy": "node config/scripts/deploy.js",
    "heroku-postbuild": "sh ./config/scripts/heroku-build.sh",
    "lint": "eslint ./src ./cypress --cache --ext .js,.jsx,.ts,.tsx --ignore-path ./.gitignore",
    "lint:tsc": "tsc --noEmit",
    "prepare": "shx cp -r ./config/git/* .git/hooks",
    "pretty": "prettier --write --ignore-path ./.gitignore ./src ./cypress && yarn lint --fix",
    "publicate:production": "yarn build --env SENTRY-production && yarn deploy production",
    "publicate:staging": "yarn build --env SENTRY-staging && yarn deploy staging",
    "serve": "yarn install && yarn webpack serve --hot --no-open --cache --bail --mode development",
    "serve:progress": "yarn serve --progress",
    "serve:public": "yarn http-server ./dist/public -p 7071 --push-state",
    "serve:static": "yarn http-server ./static -p 7073 --push-state",
    "serve:stats": "yarn serve --stats",
    "serve:stories": "yarn http-server ./dist/stories -p 7072 --push-state",
    "stories": "yarn install && yarn start-storybook -p 7074 -c config/storybook --ci",
    "tailwind": "npx tailwind-config-viewer -o",
    "test": "yarn install && shx mkdir -p ./dist/coverage && yarn test:base --maxWorkers=40% --json --outputFile=./dist/coverage/test-results.json",
    "test:base": "yarn jest --config=./config/jest/index.js -u --passWithNoTests",
    "test:base:ci": "shx mkdir -p ./dist/coverage && node --expose-gc --max-old-space-size=3072 ./node_modules/.bin/jest --config=./config/jest/index.js -u --passWithNoTests --logHeapUsage --maxWorkers=4",
    "test:ci": "yarn test:base:ci --coverageReporters=text-summary --forceExit --ci -b",
    "test:w": "yarn test --watchAll",
    "ts-coverage": "typescript-coverage-report -o ./dist/ts-coverage --ignore-files \"**/*.js?(x)\"",
    "ts-coverage:w": "npx nodemon -x \"yarn ts-coverage\" -e ts,tsx -w \"./src\" -w \"./cypress\" --ignore '{src,cypress}/**/*.{test,tests}.{ts,tsx}'",
    "up": "run-p lint lint:tsc && run-p compile && run-p \"test --coverage=false\" serve cy:run"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "@sentry/browser": "^7.21.1",
    "@sentry/tracing": "^7.21.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "classnames": "^2.3.2",
    "connected-react-router": "^6.9.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "<6.0.0",
    "react-router-dom": "<6.0.0",
    "redux": "^4.2.0",
    "redux-observable": "^2.0.0",
    "regenerator-runtime": "^0.13.11",
    "rxjs": "^7.5.7",
    "usehooks-ts": "^2.7.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.9",
    "@sentry/webpack-plugin": "^1.20.0",
    "@size-limit/preset-big-lib": "^8.1.0",
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-jest": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-postcss": "<3.0.0",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/cli": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@testing-library/cypress": "^8.0.7",
    "@types/jest": "^29.2.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-router": "^5.1.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-speech-recognition": "^3.9.0",
    "@types/storybook-readme": "^5.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "autoprefixer": "^10.4.13",
    "aws-sdk": "^2.1261.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.26.1",
    "css-loader": "^6.7.2",
    "cssnano": "^5.1.14",
    "cypress": "^11.2.0",
    "cypress-fail-fast": "^5.0.1",
    "cypress-watch-and-reload": "^1.10.5",
    "enzyme": "^3.11.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
