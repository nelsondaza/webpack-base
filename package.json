{
  "name": "webpack-base",
  "version": "1.0.0",
  "description": "Webpack base configuration",
  "main": "src/index.js",
  "keywords": [],
  "author": "Nelson Daza",
  "license": "ISC",
  "browserslist": [
    "> 1%, last 3 versions",
    "not dead",
    "not IE < 11",
    "not ExplorerMobile < 11"
  ],
  "engines": {
    "node": "^16",
    "yarn": "1.x"
  },
  "scripts": {
    "build": "yarn clear:public && yarn compile",
    "build:progress": "yarn build --progress",
    "build:sentry": "yarn build --env SENTRY",
    "build:stats": "yarn build --stats",
    "build:stories": "yarn install && yarn build-storybook -c config/storybook -o dist/stories",
    "build:w": "yarn build -w",
    "clear": "rimraf ./dist",
    "clear:public": "rimraf ./dist/public",
    "compile": "yarn install && yarn webpack --mode production --bail",
    "cy:build:open": "cypress open --env envGroup=build",
    "cy:build:run": "cypress run --env envGroup=build",
    "cy:open": "cypress open --env envGroup=dev",
    "cy:public:open": "cypress open --env envGroup=public",
    "cy:public:run": "cypress run --env envGroup=public",
    "cy:run": "cypress run --env envGroup=dev",
    "deploy": "node config/scripts/deploy.js",
    "heroku-postbuild": "sh ./config/scripts/heroku-build.sh",
    "lint": "eslint ./src ./cypress --cache --ext .js,.jsx,.ts,.tsx",
    "pre": "yarn lefthook run pre-commit && yarn lefthook run pre-push",
    "pretty": "prettier --write ./src ./cypress && yarn lint --fix",
    "publicate": "yarn build:sentry && yarn deploy",
    "serve": "yarn install && yarn webpack serve --hot --no-open --cache --bail --mode development",
    "serve:progress": "yarn serve --progress",
    "serve:public": "yarn http-server ./dist/public -p 7071 --push-state",
    "serve:static": "yarn http-server ./static -p 7073 --push-state",
    "serve:stats": "yarn serve --stats",
    "serve:stories": "yarn http-server ./dist/stories -p 7072 --push-state",
    "stories": "yarn install && yarn start-storybook -p 7074 -s ./static -c config/storybook --ci",
    "test": "yarn install && yarn test:base --maxWorkers=40% --json --outputFile=./dist/coverage/test-results.json",
    "test:base": "yarn jest --config=./config/jest/index.js -u",
    "test:base:ci": "mkdir -p ./dist/coverage && node --expose-gc ./node_modules/.bin/jest --config=./config/jest/index.js -u --logHeapUsage --maxWorkers=2",
    "test:ci": "yarn test:base:ci --coverageReporters=text-summary --forceExit --ci -b",
    "test:w": "yarn test --watchAll",
    "up": "yarn lint && yarn compile && yarn build:stories && yarn test && yarn serve"
  },
  "dependencies": {
    "@sentry/browser": "^6.14.3",
    "@sentry/tracing": "^6.14.3",
    "classnames": "^2.3.1",
    "connected-react-router": "^6.9.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2",
    "redux-observable": "^2.0.0",
    "regenerator-runtime": "^0.13.9",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@sentry/webpack-plugin": "^1.18.3",
    "@size-limit/preset-big-lib": "^6.0.4",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-jest": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.3.12",
    "@storybook/manager-webpack5": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@types/jest": "^27.0.2",
    "@types/js-yaml": "^4.0.4",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "ansi-regex": "^6.0.1",
    "autoprefixer": "^10.4.0",
    "aws-sdk": "^2.1028.0",
    "copy-webpack-plugin": "^9.1.0",
    "core-js": "^3.19.1",
    "css-loader": "^6.5.1",
    "cssnano": "^5.0.10",
    "cypress": "^9.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.4.0",
    "http-server": "^14.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^11.2.6",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^2.4.4",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "prettier": "^2.4.1",
    "react-refresh": "^0.11.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^3.0.2",
    "s3-upload-stream": "^1.0.7",
    "sass-loader": "^12.3.0",
    "sass-resources-loader": "^2.2.4",
    "size-limit": "^6.0.4",
    "storybook-readme": "^5.0.9",
    "style-loader": "^3.3.1",
    "tailwindcss": "^2.2.19",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.5.0",
    "workbox-webpack-plugin": "^6.3.0"
  }
}
